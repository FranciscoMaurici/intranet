{
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "build:analyze": "ANALYZE=true yarn build",
    "start": "next start",
    "type-check": "tsc",
    "test": "TZ=UTC jest --config ./jest.config.ts",
    "test:ci": "TZ=UTC jest --config ./jest.config.ts --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --watchAll=false",
    "lint": "DEBUG=eslint:cli-engine eslint \"**/*.{ts,tsx,d.ts,md,mdx}\"",
    "lint:fix": "eslint \"**/*.{ts,tsx,d.ts,md,mdx}\" --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "pretest": "./node_modules/.bin/eslint --ignore-path .gitignore . --fix",
    "install:e2e": "cd e2e/testcafe; yarn install",
    "install:electron": "cd _extensions/desktop-app; yarn install",
    "postinstall": "husky install",
    "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\" && npx husky add .husky/pre-push \"yarn test\"",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "storybook:test": "test-storybook",
    "swagger": "yarn swagger:typescript2yaml && yarn swagger:yamlassemble",
    "swagger:typescript2yaml": "node swagger.js && typeconv -f ts -t oapi -o ./.openapi -O .swagger.yaml --no-hidden --strip-annotations '.openapi/alltypes.ts' ",
    "swagger:yamlassemble": "next-swagger-doc-cli ./.next-swagger-doc.json --output ./src/models/swagger.json",
    "prisma": "prisma",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:ci": "cypress run --config video=true",
    "cy:server:ci": "start-server-and-test start http://localhost:3000 cy:run:ci"
  },
